---
title: Independent Space Index 2021
layout: default
---

<div class="spaces-container">
    <ul class="spaces">
    {% for space in spaces %}
        <div class="space" id="{{space.spaceData.uid}}">
            <h1 class="space__title">{{ space.spaceName }}</h1>
            <address class="space__address">
                <i class="la la-map-marker"></i> {{space.spaceData.address}}, {{ space.spaceData.district | plz }} Vienna<br>
                <i class="las la-external-link-alt"></i> <a href="{{ space.spaceData.website }}" target="_blank">{{ space.spaceData.prettyurl }}</a>
            </address>
            <br>
            <h2 class="space__event-title">{{space.eventTitle}}</h2>
            <p>{{ space.eventDetails }}</p>
        </div>
    {% endfor %}
    </ul>
</div>